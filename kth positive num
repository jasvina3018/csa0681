
def findKthPositive(arr, k):
    missing = []
    num = 1
    while len(missing) < k:
        if num not in arr:
            missing.append(num)
        num += 1
    return missing[-1]
# Example 1
arr1 = [2, 3, 4, 7, 11]
k1 = 5
output1 = findKthPositive(arr1, k1)
print(output1)  
# Example 2
arr2 = [1, 2, 3, 4]
k2 = 2
output2 = findKthPositive(arr2, k2)
print(output2)
